  <!--<rosparam param="search_area">[[-2.0,4.0],[4.0,4.0],[4.0,-6.0],[-2.0,-6.0]]</rosparam>-->
  
  <!-- simulated lab -->
  <rosparam param="search_area">[[2.0,-8.0],[5.0,-8.0],[5.0,2.0],[2.0,2.0]]</rosparam>
 
  <!-- real bham lab -->
